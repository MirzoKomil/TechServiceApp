            android:exported="false" />
        <activity
            android:name="com.coworking.texxizmat.CheckingActivity"
            android:exported="false" />
        <activity
            android:name="com.coworking.texxizmat.GasActivity"
            android:exported="false" />
        <activity
            android:name="com.coworking.texxizmat.SearchActivity"
            android:exported="false" />
        <activity
            android:name="com.coworking.texxizmat.SignatureActivity"
            android:exported="false"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity
            android:name="com.coworking.texxizmat.MainMenuActivity"
