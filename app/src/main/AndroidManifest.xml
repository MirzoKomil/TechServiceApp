        android:supportsRtl="true"
        android:theme="@style/Theme.TexXizmat"
        tools:targetApi="31">
        <activity
            android:name="com.coworking.texxizmat.OkActivity"
            android:exported="false" />
        <activity
            android:name="com.coworking.SqlActivity"
            android:exported="false"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.coworking.texxizmat.login.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.coworking.texxizmat.login.RegActivity"
            android:exported="false" />
        <activity
            android:name="com.coworking.texxizmat.ServisActivity"
