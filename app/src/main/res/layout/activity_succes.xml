                        android:layout_marginEnd="16dp"
                        android:fontFamily="@font/robotoflex_variable"
                        android:text="Gaz jihozlari standartlarga javob beradi"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/sa_relativeLayout3" />

