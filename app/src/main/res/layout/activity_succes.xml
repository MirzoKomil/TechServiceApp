                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/sa_checkBox9" />


            <LinearLayout
                android:id="@+id/ll_price"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintBottom_toTopOf="@id/sa_cancel_btn"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/sa_checkBox10"
                app:layout_constraintVertical_bias="0.191">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="40dp"
                    android:layout_marginTop="20dp"
                    android:fontFamily="@font/robotoflex_variable"
                    android:text="Narx: "
                    android:textColor="#AFAFAF" />

                <EditText
                    android:id="@+id/et_price"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="40dp"
                    android:layout_marginTop="20dp"
                    android:fontFamily="@font/robotoflex_variable"
                    android:hint="Xizmat narxini kiriting"
                    android:textColor="#000" />


            </LinearLayout>

            <ImageView
                android:id="@+id/signature_image_view"
                android:layout_width="200dp"
                android:layout_height="100dp"
                app:layout_constraintBottom_toTopOf="@id/sa_cancel_btn"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/ll_price" />


            <!-- New TextViews for displaying checkbox states -->

            <TextView
                android:id="@+id/elektron_imzo"
                android:layout_width="150dp"
                android:layout_height="match_parent"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="24dp"
                android:fontFamily="@font/robotoflex_variable"
                android:text="Imzo"
                android:textAlignment="center"
                android:textSize="25sp"
                android:textStyle="bold"
                android:textColor="@color/blue"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/signature_image_view"
                app:layout_constraintVertical_bias="0.0" />


            <TextView
