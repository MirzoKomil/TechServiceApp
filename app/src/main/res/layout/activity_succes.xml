                        android:fontFamily="@font/robotoflex_variable"
                        android:text="Sizning joylashuvingiz aniqlandi (tez kunda)"
                        android:textColor="#CACACA"
                        android:textSize="14sp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.498"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tvReestr" />

                    <RelativeLayout
                        android:id="@+id/sa_relativeLayout"
                        android:layout_width="346dp"
                        android:layout_height="315dp"
                        android:layout_marginTop="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/sa_textView">

                        <ImageView
                            android:id="@+id/sa_foto_big"
                            android:layout_width="match_parent"
                            android:layout_height="453dp"
                            android:background="@drawable/solid" />


                    </RelativeLayout>

                    <TextView
