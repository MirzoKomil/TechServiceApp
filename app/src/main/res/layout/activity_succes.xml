                        app:layout_constraintTop_toBottomOf="@+id/sa_checkedText5" />

                    <TextView
                        android:id="@+id/sa_checkedText6"
                        android:layout_width="370dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/sa_checkBox8"/>



                    <CheckBox
                        android:id="@+id/sa_checkBox9"
