<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.coworking.texxizmat.CheckingActivity">

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar" />



    <ImageView
        android:id="@+id/ll_logo"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        app:layout_constraintTop_toBottomOf="@id/toolbar"
        android:layout_marginTop="30dp"
        android:src="@drawable/checking"
        android:contentDescription="@string/todo" />


    <ListView
        android:id="@+id/lv_workProcess"
        android:layout_width="match_parent"
        android:layout_height="196dp"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@+id/ll_logo"/>

    <ListView
        android:layout_width="match_parent"
        android:layout_height="196dp"
        android:layout_marginTop="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lv_workProcess" />


</androidx.constraintlayout.widget.ConstraintLayout>